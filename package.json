{
  "name": "no-framework",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "debug": "concurrently \"npm run server:debug\" \"npm run client\"",
    "build": "node fuse build",
    "postinstall": "npm run build",
    "dist": "concurrently \"npm run server\" \"node fuse dist\"",
    "client": "node fuse",
    "server": "nodemon src/server/index.js",
    "server:debug": "nodemon --inspect-brk src/server/index.js",
    "test": "mocha --require @babel/register --require @babel/polyfill --recursive --reporter spec",
    "cover": "nyc --reporter=lcov node_modules/mocha/bin/_mocha --require babel-register --recursive --reporter spec"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-istanbul": "^0.12.2",
    "chai": "^4.2.0",
    "concurrently": "^4.0.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "fuse-box": "^3.5.0",
    "http-proxy-middleware": "^0.19.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "nyc": "^13.0.1",
    "sinon": "^6.3.4",
    "sinon-chai": "^3.2.0",
    "typescript": "^2.9.2",
    "uglify-es": "^3.3.10",
    "uglify-js": "^3.4.9"
  },
  "dependencies": {
    "rw-progress": "^1.0.3",
    "rw-router": "^1.1.0",
    "rw-templater": "^1.1.2"
  }
}
